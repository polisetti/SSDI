          <Item Value="Dateadded"/>
          <Item Value="Portfolioid"/>
          <Item Value="Stockid"/>
          <Item Value="Timeadded"/>
          <Item Value="Selection"/>
        </AttrNames>
      </nodeDefinition>
    </tree>
    <methodAction id="compareStocks"
